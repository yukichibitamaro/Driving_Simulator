pipeline {
    agent {label 'master'}
    stages {
        
        stage('Checkout') {
            steps {
                // GitHubリポジトリをチェックアウトするステップ
                checkout([$class: 'GitSCM', branches: [[name: '*/feature/CItest']],
                          userRemoteConfigs: [[url: 'https://github.com/yukichibitamaro/Driving_Simulator']]])
            }
        }
        
        stage('Build') {
            steps {
                echo ${env.GIT_COMMIT}
            }
        }
    }
}
